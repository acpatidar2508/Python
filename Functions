def add(a,b):
    return(a+b)
print(add(10,20))

def sub(a,b):
    return(a-b)
print(sub(10,20))

def sub(a,b):
    result=a-b
    return result
print(sub(50,20))

abc=999
def f1():
    print(abc+100)
    return ""


def f2():
    global abc
    abc=900
    print(abc+800)
    return ""

def f3():
    print(abc)
    return ""

print(f1())
print(f2())
print(f3())

def outer():
    print("inside")
    def inner():
        return "inside"
        print("hi")
    return "outer", inner()

print(outer())

def outer(a,b):
    def inner(c,d):
        return c-d+a+b
        print("inner func")
    return 100, inner(10,20)

print(outer(5,15))


