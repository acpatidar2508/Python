import math
# print(help(math))
print(math.factorial(5))
print(math.sqrt(5))
print(math.sin(90))
print(math.ceil(5.4))
print(math.floor(5.2))
print(math.pi)
print(math.e)

import random
print(random.random())

for i in  range(5):
    print(random.random())


for i in  range(3):
    print("randint", random.randint(1000,9999))

for i in  range(6):
    print("randint", random.randint(1,6))

for i in  range(6):
    print("uniform", random.uniform(1,6))


