s1={1, 2, 3, 5, 8, 9, 10, 'adi'}
s2={1,2,9,8,3,26,52,46,85}
s3={100}
print(s1,s2,s3)

print(s1.intersection(s2))
print(s1.intersection(s3))

print(s1.union(s2))
print(s1.union(s3))

print(s1.difference(s2))
print(s2.difference(s1))
print(s3.difference(s1))
print(s1.difference(s3))



