for i in d:
    print(i)

for i in d.keys():
    print(i)

for i in d.values():
    print(i)

for i in d.items():
    print(i)

print(d.items())
print(d.keys())
print(d.values())

for keys in d.items():
    print(keys)

for values in d.items():
    print(values)

for keys,values in d.items():
    print(keys,values)


